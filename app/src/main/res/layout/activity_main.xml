<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">

    <View
        android:id="@+id/view"
        android:layout_width="553dp"
        android:layout_height="252dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:text="Рассчитанная матрица:"
        android:textAlignment="center"
        android:textColor="#303030"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.376" />

    <TableLayout
        android:layout_width="300sp"
        android:layout_height="300sp"
        android:layout_marginTop="256dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.366">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/one"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:text="1"
                android:textAlignment="center"
                android:textColor="#303030"
                android:textSize="34sp" />

            <TextView
                android:id="@+id/four"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:text="4"
                android:textAlignment="center"
                android:textColor="#303030"
                android:textSize="34sp" />

            <TextView
                android:id="@+id/seven"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:text="7"
                android:textAlignment="center"
                android:textColor="#303030"
                android:textSize="34sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/two"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:includeFontPadding="true"
                android:text="2"
                android:textAlignment="center"
                android:textColor="#303030"
                android:textSize="34sp" />

            <TextView
                android:id="@+id/five"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:text="5"
                android:textAlignment="center"
                android:textColor="#303030"
                android:textSize="34sp" />

            <TextView
                android:id="@+id/eight"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:text="8"
                android:textAlignment="center"
                android:textColor="#303030"
                android:textSize="34sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/three"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:text="3"
                android:textAlignment="center"
                android:textColor="#030300"
                android:textSize="34sp" />

            <TextView
                android:id="@+id/six"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:text="6"
                android:textAlignment="center"
                android:textColor="#030300"
                android:textSize="34sp" />

            <TextView
                android:id="@+id/nine"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:text="9"
                android:textAlignment="center"
                android:textColor="#030300"
                android:textSize="34sp" />
        </TableRow>

    </TableLayout>

    <EditText
        android:id="@+id/birth"
        android:layout_width="345dp"
        android:layout_height="58dp"
        android:ems="10"
        android:fontFamily="sans-serif-light"
        android:hint="Дата"
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#454545"
        android:textColorHighlight="#FF5C04"
        android:textColorHint="#454545"
        android:textColorLink="#454545"
        android:textSize="30sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.393"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.078"
        tools:ignore="HardcodedText,LabelFor" />

    <TextView
        android:id="@+id/Label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:text="Нумерология 888"
        android:textColor="#303030"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.018" />

    <EditText
        android:id="@+id/date2"
        android:layout_width="339dp"
        android:layout_height="56dp"
        android:ems="10"
        android:fontFamily="sans-serif-light"
        android:hint="Дата"
        android:inputType="date"
        android:textAlignment="center"
        android:textColor="#303030"
        android:textColorHint="#303030"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.459"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.16" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/calc"
        android:layout_width="253dp"
        android:layout_height="55dp"
        android:background="@drawable/gradient"
        android:fontFamily="sans-serif-light"
        android:text="Рассчитать"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.271" />

</androidx.constraintlayout.widget.ConstraintLayout>